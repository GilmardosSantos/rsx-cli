{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "fileExtension": {
            "type": "string",
            "enum": ["ts", "js"]
        },
        "sheetFileExtension": {
            "type": "string",
            "enum": ["scss", "sass", "css", "none"]
        },
        "testFileExtension": {
            "type": "string",
            "enum": ["spec", "test"]
        },
        "componentType": {
            "type": "string",
            "enum": ["function", "class"]
        }
    }
}